{
  "name": "nextjs",
  "version": "1.0.0",
  "description": "nextjs-with-styled-component",
  "author": "demian",
  "main": "index.js",
  "repository": "git@github.com:impressor615/demian.git",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "tslint -c tslint.json -e './node_modules/**/*' './**/*.ts?(x)'",
    "prepush": "npm run test && npm run lint",
    "push": "git push",
    "analyze:server": "BUNDLE_ANALYZE=server yarn build",
    "analyze:browser": "BUNDLE_ANALYZE=browser yarn build",
    "analyze": "BUNDLE_ANALYZE=both yarn build"
  },
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^24.0.11",
    "@types/next": "^8.0.4",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "jest": "^24.7.1",
    "next": "^8.1.0",
    "next-compose-plugins": "^2.1.1",
    "next-images": "^1.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)": "<rootDir>/$1"
    }
  }
}
